<?xml version='1.0' encoding='utf-8'?>
<widget id="dev.enginegroup.bemoreparrot" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Be More Parrot - Engine Game Jam 19</name>
  <description>
    Be more parrot a game built at Engine Game Jam 19.
  </description>
  <author email="game-jam@enginegroup.com" href="http://enginegroup.dev">
    Engine Game Jam Team
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <preference name="Orientation" value="landscape"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <preference name="BackgroundColor" value="0xff000000"/>
  <plugin name="cordova-plugin-whitelist" spec="^1.3.3"/>
  <plugin name="cordova-plugin-console" spec="^1.1.0"/>
  <plugin name="cordova-plugin-dialogs" spec="^2.0.1"/>
  <plugin name="cordova-plugin-globalization" spec="^1.0.9"/>
  <engine name="windows"/>
  <engine name="ios" spec="^4.5.4"/>
  <engine name="android" spec="^6.4.0"/>
</widget>
